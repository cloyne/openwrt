
config wifi-device 'radio0'
	option type 'mac80211'
	option macaddr 'macAddressPlaceholder'
	option hwmode '11ng'
	option htmode 'HT20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'DSSS_CCK-40'
	option country 'US'
	option txpower '20'
	option channel 'auto'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'Cloyne'
	option encryption 'psk2'
	option key 'Cloyne is suck'

